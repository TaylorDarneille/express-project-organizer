<h1>Create a new project</h1>

<form action="/projects" method="POST">
  <fieldset>
    <legend>New Project</legend>
    <div class="row">
      <div class="col-sm-12 col-md-6">
        <label for="name">Name</label>
        <input type="text" class="form-control" id="name" name="name" required>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-6">
        <label for="githubLink">Github Link</label>
        <input type="text" class="form-control" id="githubLink" name="githubLink" required>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-6">
    <label for="deployedLink">Deployed Link</label>
    <input type="text" class="form-control" id="deployedLink" name="deployedLink" required>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-6">
    <label for="description">Description</label>
    <textarea type="text" class="form-control" id="description" name="description" required></textarea>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 col-md-6">
    <label for="category">Category</label>
    <input type="category" class="form-control" id="category" name="category" required>
      </div>
    </div>
    <input type="submit" class="button primary">
  </fieldset>
</form>
