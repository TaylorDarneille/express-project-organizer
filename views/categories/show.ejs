<h1><%= categories.name %></h1>
<h2>Project Category: <%= categories.name %></h2>

<p><%= categories.name %></p>

<% categories.projects.forEach((project)=> { %>
    <div class="well">
      <h2>
        <a href="/projects/<%= project.ProjectCategory.projectId %>"><%= project.name %></a>
      </h2>
    </div>
  <% }); %>


<hr />